@{
    ViewData["Title"] = "首頁";
}

<!-- Carousel Start -->
<div class="container-fluid p-0 mb-5 wow fadeIn" data-wow-delay="0.1s">
    <div id="header-carousel" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item">
                <img class="w-100" src="~/img/carousel-7.jpg" alt="Image">
                <div class="carousel-caption">
                    <div class="container">
                        <div class="row justify-content-start">
                            <div class="col-lg-7 ms-md-3 ms-lg-3">
                                <h1 class="display-2 mb-5 animated slideInDown text-primary ms-md-5 ms-lg-5 momo-slogan">生活要過、<br />甜點配酒！</h1>
                                @*<p class="mb-4 text-dark">這個放本店廣告，旁邊放logo</p>*@
                                <a asp-controller="Home" asp-action="Class" class="btn btn-primary rounded-pill py-sm-3 px-sm-5  ms-md-5 ms-lg-5">甜點手作坊</a>
                                @*<a href="" class="btn btn-secondary rounded-pill py-sm-3 px-sm-5 ms-3">這個考慮拔掉</a>*@
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="carousel-item active">
                <img class="w-100" src="~/img/carousel-1.jpg" alt="Image">
                <div class="carousel-caption">
                    <div class="container">
                        <div class="row justify-content-start">
                            <div class="col-lg-7 ms-md-3 ms-lg-3">
                                <h1 class="display-2 mb-5 animated slideInDown text-primary ms-md-5 ms-lg-5 momo-slogan">生活要過、 <br />甜點配酒！</h1>
                                @*<p class="mb-4 text-dark">這個放廣告</p>*@
                                <a asp-controller="Home" asp-action="DemoProduct"
                                   class="btn btn-primary rounded-pill py-sm-3 px-sm-5 ms-md-5 ms-lg-5">甜點品項</a>
                                @*<a href="" class="btn btn-secondary rounded-pill py-sm-3 px-sm-5 ms-3">這要寫啥好呢</a>*@
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="carousel-item">
                <img class="w-100" src="~/img/carousel-3.jpg" alt="Image">
                <div class="carousel-caption">
                    <div class="container">
                        <div class="row justify-content-start">
                            <div class="col-lg-7 ms-md-3 ms-lg-3">
                                <h1 class="display-2 mb-5 animated slideInDown text-primary ms-md-5 ms-lg-5 momo-slogan">生活要過、 <br />甜點配酒！</h1>
                                @*<p class="mb-4 text-dark">這個放廣告</p>*@
                                <a asp-controller="Home" asp-action="Blog"
                                   class="btn btn-primary rounded-pill py-sm-3 px-sm-5 ms-md-5 ms-lg-5">甜嘴MoBlog</a>
                                @*<a href="" class="btn btn-secondary rounded-pill py-sm-3 px-sm-5 ms-3">這要寫啥好呢</a>*@
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="carousel-item">
                <img class="w-100" src="~/img/carousel-2.jpg" alt="Image">
                <div class="carousel-caption">
                    <div class="container">
                        <div class="row justify-content-start">
                            <div class="col-lg-7 ms-md-3 ms-lg-3">
                                <h1 class="display-2 mb-5 animated slideInDown text-primary ms-md-5 ms-lg-5 momo-slogan">生活要過、 <br />甜點配酒！</h1>
                                @*<p class="mb-4 text-dark">這個放廣告</p>*@
                                <a asp-controller="Home" asp-action="orderCustomizedCake" class="btn btn-primary rounded-pill py-sm-3 px-sm-5 ms-md-5 ms-lg-5">客製化蛋糕</a>
                                @*<a href="" class="btn btn-secondary rounded-pill py-sm-3 px-sm-5 ms-3">這要寫啥好呢</a>*@
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#header-carousel" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#header-carousel" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>
</div>
<!-- Carousel End -->
<!-- About Start -->
<div class="container-xxl py-5">
    <div class="container">
        <div class="row g-5 align-items-center">
            <div class="col-lg-5 col-md-6 wow fadeIn" data-wow-delay="0.1s">
                <div class="about-img position-relative overflow-hidden p-5 pe-0">
                    <img class="img-fluid w-100" src="img/MOＭＯ.png" />
                </div>
            </div>
            <div class="col-lg-7 col-md-6 wow fadeIn" data-wow-delay="0.5s">
                <h1 class="display-5 mb-4 text-primary">甜點師Momo</h1>
                <p class="mb-4 display-6 text-secondary">嘴裡吃點甜讓生活帶些甜</p>
                <p class="mb-4">因長期自體免疫狀況及經常在小兒科打滾，深知原型食材對我們的重要性。不僅使用原食材，也堅持好食材，來呈現天然的美味。</p>
                <p><i class="fa fa-check text-primary me-3"></i>各式果醬均是親自熬煮</p>
                <p><i class="fa fa-check text-primary me-3"></i>不使用防腐劑、香精、膨鬆劑…等化學食品添加</p>
                <p><i class="fa fa-check text-primary me-3"></i>造型蛋糕裝飾調色使用少許食用色素</p>
                @*<a class="btn btn-primary rounded-pill py-3 px-5 mt-3" href="">MENU</a>*@
                <a asp-controller="Home" asp-action="DemoProduct"
                   class="btn btn-primary rounded-pill py-3 px-5 mt-3">甜點品項</a>
            </div>
        </div>
    </div>
</div>
<!-- About End -->
<!-- Feature Start -->
<div class="container-fluid bg-light bg-icon my-5 py-6">
    <div class="container">
        <div class="section-header text-center mx-auto mb-5 wow fadeInUp" data-wow-delay="0.1s"
             style="max-width: 500px;">
            <h1 class="display-5 mb-3 text-t3">最新消息</h1>
        </div>
        <div class="row g-4">
            <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
                <div class="bg-white text-center h-100 p-4 p-xl-5">
                    <img class="img-fluid mb-4 momo-w90" src="img/icon-1.png" alt="">
                    <h4 class="mb-3  text-body">最新活動</h4>
                    <p class="mb-4">最新活動與課程</p>
                    <a asp-controller="Home" asp-action="Class"
                       class="btn btn-outline-primary border-2 py-2 px-4 rounded-pill">甜點課程</a>
                </div>
            </div>
            <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.3s">
                <div class="bg-white text-center h-100 p-4 p-xl-5">
                    <img class="img-fluid mb-4" src="img/icon-2.png" alt="">
                    <h4 class="mb-3 text-body">客製化蛋糕</h4>
                    <p class="mb-4">訂購蛋糕</p>
                    <a asp-controller="Home" asp-action="orderCustomizedCake"
                       class="btn btn-outline-primary border-2 py-2 px-4 rounded-pill">前往訂購</a>
                </div>
            </div>
            <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.5s">
                <div class="bg-white text-center h-100 p-4 p-xl-5">
                    <img class="img-fluid mb-4" src="~/img/icon-3.png" alt="">
                    <h4 class="mb-3 text-body">最新消息</h4>
                    <p class="mb-4">Blog文章</p>
                    <a asp-controller="Home" asp-action="Blog"
                       class=" btn-outline-primary btn border-2 py-2 px-4 rounded-pill">甜嘴MoBlog</a>
                </div>
            </div>

        </div>
    </div>
</div>
<!-- Feature End -->
